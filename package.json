{
  "name": "troposphere",
  "description": "Atmosphere Web Client",
  "version": "0.0.1",
  "author": "iPlant Collaborative",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/iPlantCollaborativeOpenSource/troposphere",
  "repository": {
    "type": "git",
    "url": "git://github.com/iPlantCollaborativeOpenSource/troposphere.git"
  },
  "bugs": {
    "url": "https://github.com/iPlantCollaborativeOpenSource/troposphere/issues"
  },
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "dev": "webpack -d --progress --profile --colors --bail --",
    "build": "webpack --progress --profile --colors --bail --",
    "watch": "webpack --progress --profile --colors --bail --watch --",
    "jenkins": "webpack --profile --colors --",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stats": "webpack --profile --json > stats.json",
    "lint": "eslint --ignore-pattern './troposphere/static/js/lib/**/*.js' troposphere/static/js",
    "lint-pre-commit": "eslint --stdin",
    "format-check": "esformatter -c .esformatter --diff $(git diff --diff-filter=M --name-only | grep js$) || exit 0",
    "format": "esformatter -c .esformatter -i $(git diff --diff-filter=M --name-only | grep js$)"
  },
  "dependencies": {
    "babel-runtime": "^5.8.12",
    "backbone": "^1.2.1",
    "bootstrap-sass": "^3.3.6",
    "crypto-js": "^3.1.5",
    "d3": "^3.5.6",
    "highcharts-commonjs": "^4.1.7-1",
    "jquery": "^2.1.3",
    "jstz": "^1.0.7",
    "moment": "^2.10.6",
    "moment-timezone": "^0.4.0",
    "q": "^1.4.1",
    "raven-js": "^3.0.4",
    "react": "^0.13.3",
    "react-router": "^0.13.0",
    "react-tools": "^0.13.0",
    "showdown": "^1.2.1",
    "toastr": "^2.1.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^5.8.14",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "clean-webpack-plugin": "^0.1.3",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.15.6",
    "esformatter": "^0.9.3",
    "esformatter-eol-last": "^1.0.0",
    "esformatter-jsx": "^5.0.4",
    "esformatter-quotes": "^1.0.3",
    "eslint": "^1.4.3",
    "eslint-plugin-react": "^3.14.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.3",
    "node-sass": "^3.2.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "sass-loader": "^1.0.3",
    "sinon": "~1.10.3",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.5",
    "webpack-bundle-tracker": "0.0.93",
    "yuglify": "~0.1.4"
  }
}
